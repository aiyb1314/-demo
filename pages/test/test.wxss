.testContainer {
  display: flex;
  flex-flow: nowrap;
  height: 80rpx;
}

.cityCss{
  padding-left: 30rpx;
  margin-bottom: 30rpx;
  margin-right: 30rpx;
  line-height: 80rpx;
}

.sContainer{
  display: flex;
  background-color:rgb(252,252,252);
  width: 60%;
}

.containerView{
    line-height: 60rpx;
    background-color: rgb(7,193,96);
    color: white;
    height: 60rpx;
    margin-top: 10rpx;
    width: 100rpx;
    text-align: center;
    border-radius: 30rpx;
}

.routerInput{
    height: 80rpx;
    margin-left: 10rpx;
}

.dis{
  display: none;
}

.middle {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    height: 80rpx;
}

.middle .middleItem{
  line-height: 80rpx;
  font-size: 28rpx;
}

.action {
  border-bottom: red 3rpx solid;
}

.footer{
  width: 700rpx;
  height: 80rpx;
  background-color:rgb(242,241,237);
  border: 2rpx solid rgb(242,241,237);
  border-radius: 20rpx;
  margin: 0 auto;
}

.footer .swiperItem{
  text-align: center;
  line-height: 80rpx;
}

.orderBtn{
    background-color: red;
    border-radius: 20rpx;
    width: 400rpx;
    height: 60rpx;
    color: white;
    line-height: 60rpx;
    margin-left: 180rpx;
    text-align: center;
    margin-top: 10rpx;
}


.icon{
    text-align: center;
}


.icon-sousuo{
  font-size: 40rpx;
  line-height: 80rpx;
  padding-top: 4rpx;
}

.orderParking{
  margin-top: 30rpx;
}